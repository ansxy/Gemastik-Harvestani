{
	"arrowParens": "always",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"singleQuote": true,
	"semi": false,
	"printWidth": 100,
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"importOrder": [
		"^@react|@react-native/(.*)$",
		"^@react-navigation/native-stack/(.*)$",
		"<THIRD_PARTY_MODULES>",
		"^@/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
